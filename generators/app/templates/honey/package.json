{
  "name": "<%= packageName %>",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "chinaza",
  "private": true,
  "scripts": {
    "start:dev": "nodemon --exec ts-node --transpile-only ./src/index.ts",
    "build": "npx tsc",
    "generate:migration": "npx sequelize-cli migration:generate --name",
    "migrate:up": "npx sequelize-cli db:migrate --url",
    "migrate:down": "npx sequelize-cli db:migrate:undo --url",
    "generate:model": "npx sequelize-cli model:generate",
    "generate:seed": "npx sequelize-cli seed:generate --name",
    "seed:up": "npx sequelize-cli db:seed:all --url",
    "seed:down": "npx sequelize-cli db:seed:undo:all --url",
    "lint-staged": "lint-staged",
    "test": "jest --passWithNoTests"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.606.0",
    "@aws-sdk/s3-request-presigner": "^3.606.0",
    "@chinaza/auth": "^1.7.10",
    "@chinaza/honey": "^1.45.1",
    "@chinaza/scorp-backend-api": "^4.8.1",
    "@sentry/node": "7.106.0",
    "dotenv": "^16.0.3",
    "firebase-admin": "^11.11.0",
    "joi": "^17.12.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-fetch": "^2.6.11",
    "qs-stringify": "^1.2.1",
    "redis": "^4.6.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@chinaza/eslint-config": "^2.0.5",
    "@types/jest": "^29.2.6",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^24.3.1",
    "@types/uuid": "^9.0.2",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "^3.5.3",
    "sequelize-cli": "^6.6.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.40.0"
  },
  "packageManager": "yarn@4.9.4"
}
